{
  "manifest_version": 2,
  "name": "IoT to Scrotum",
  "version": "1.0",
  "description": "Replaces the text 'IoT' with 'scrotum'.",
  "content_scripts":
  [
    {
      "matches": ["*://*/*"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}
